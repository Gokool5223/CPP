    {
     int n,i,a,sum=0;
     clrscr();
     printf("Enter number...");
     scanf("%d",&n);
     printf("Enter array elements :\n" );
     for(i=0;i<n;i++)
      {
        printf("Enter element %d : ",i+1);
        scanf("%d",&a);
      }
     
     for(i=0;i<n;i++)
      sum=sum+a;
     printf("\nSum of given array elements is : %d",sum);
     getch();
