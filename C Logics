    #include<stdio.h>
    #include<conio.h>  
    main()
    {
      int i,j,m,a,sum=0;
      clrscr();
      printf("Enter order of matrix :\n");
      scanf("%d",&m);
      printf("Enter matrix elements.....\n");
      for(i=0;i<m;i++)
        for(j=0;j<m;j++)
          scanf("%d",&a);
     
      for(i=0;i<m;i++)
        for(j=0;j<m;j++)
          if(i<j)
            sum=sum+a;
     
      printf("\nSum of upper triangular elements.....%d",sum);
      getch();
    }


