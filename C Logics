    #include<conio.h>  
    #include<string.h>
    main()
    {
        char *s1,*s2;
        clrscr();
        printf("Enter string : ");
        scanf("%s",s1);
        printf("Reverse String : %s",strrev(s1));
        printf("Lenght: %d",strlen(s1));
        printf("String in UPPER CASE : %s",strupr(s1));
        printf("String in lower case : %s",strlwr(s1));
        strcpy(s2,s1);
        printf("\nCopied string is...%s",s2);
        getch();
    }

