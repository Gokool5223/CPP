      clrscr();
      printf("Enter order of matrix :\n");
      scanf("%d%d",&m,&n);
      printf("Enter matrix elements of first matrix.....\n");
      for(i=0;i<m;i++)
        for(j=0;j<n;j++)
          scanf("%d",&a);
     
      printf("Enter matrix elements of second matrix.....\n");
      for(i=0;i<m;i++)
        for(j=0;j<n;j++)
          scanf("%d",&b);
     
      for(i=0;i<m;i++)
        for(j=0;j<n;j++)
          c;
     
      printf("Resultant matrix .....\n");
      for(i=0;i<m;i++)
      {
        for(j=0;j<n;j++)
          printf("%d ",c);
        printf("\n");
      }
      getch();
    }

